<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qimiao.dao.CustMsgDao">
	<insert id="addCustMsg" parameterType="com.qimiao.bean.CustMsg">
		INSERT into cust_msg (customer_name,customer_phone,customer_email,customer_need,time)
VALUES(#{custMsg.customerName},#{custMsg.customerPhone},#{custMsg.customerEmail},#{custMsg.customerNeed},#{custMsg.time})
	</insert>


</mapper>